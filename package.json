{
  "name": "clarito",
  "version": "0.1.0",
  "private": true,
  "main": "electron.js",
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "@types/react": "^15.0.13",
    "electron": "^1.6.0",
    "netlify": "^1.2.0",
    "node-sass": "^4.5.0",
    "react-icons": "^2.2.3",
    "react-scripts": "0.9.2"
  },
  "dependencies": {
    "platform": "^1.3.3",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "scripts": {
    "start": "react-scripts start & npm run styles",
